{
    "name": "quicktype",
    "displayName": "Paste JSON as Code",
    "description": "Copy JSON, paste as Go, TypeScript, C#, C++ and more.",
    "version": "0.0.4",
    "publisher": "quicktype",
    "icon": "media/icon.png",
    "galleryBanner": {
        "color": "#00212b",
        "theme": "dark"
    },
    "homepage": "https://quicktype.io",
    "author": {
        "name": "quicktype.io",
        "email": "hello@quicktype.io"
    },
    "bugs": {
        "url": "https://github.com/quicktype/quicktype/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/quicktype/quicktype"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "json",
        "converter"
    ],
    "markdown": "github",
    "activationEvents": [
        "onCommand:extension.pasteJSONAsCode",
        "onCommand:extension.pasteJSONForSerialization"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.pasteJSONAsCode",
                "title": "Paste JSON as code"
            },
            {
                "command": "extension.pasteJSONForSerialization",
                "title": "Paste JSON for serialization"
            }
        ]
    },
    "scripts": {
        "patch-version": "ts-node script/patch-npm-version.ts",
        "publish": "npm run patch-version && vsce publish -p $VSCE_TOKEN",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/copy-paste": "^1.1.30",
        "@types/node": "^8.5.2",
        "compare-versions": "^3.1.0",
        "shelljs": "^0.7.8",
        "ts-node": "^4.1.0",
        "typescript": "^2.5.3",
        "vsce": "^1.35.0",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "quicktype": "^7.0.1"
    }
}
